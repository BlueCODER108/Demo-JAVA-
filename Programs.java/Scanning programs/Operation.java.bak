import java.util.Scanner;
class Operation
{
	public static void main(String[] args) 
	{
		Scanner sc = new Scanner(System.in);
		System.out.println("The a Value: ");
		int a= sc.nextInt();
		int b= a%10;
		int c= (a/1000)%10;
		int d= b+c;
		System.out.println("The last digit of the value is: " + b);
		System.out.println("The Second digit of the value is: " + c);
		System.out.println("The Sum of the second and last digit number is: " + d);
	}
}
